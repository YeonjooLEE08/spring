@charset "UTF-8";
*{ 
margin:0; 
padding:0; 
list-style:none; 
box-sizing:border-box; } 

a{
	text-decoration:none;
	color:inherit;
} 
 .clear{
	clear:both;
} 

.menuTitle{
	height: 130px;
	background-color:  #E7EDE4;
	text-align: center;
	font-weight: 700;
	font-size:25px;
	padding-left:0px;
	color:black;
}
#nav-v1{
	width:100%;
 	text-align:center;
 	padding: 0px;
} 
.menu-v1{
	line-height:2; 
	border: 1px solid #dddddd;
	border-top:none;
} 

.menu-v1:hover{
	background-color:#16784B;
} 
.menu-v1:hover>a{
	color:white;
	font-weight:bold;
} 
.menu-v1 .submenu{
	margin-top:7px; 
	display:none; 
	padding:0px;
	} 
	
.menu-v1 .submenu li:hover{ 
	background-color:#16784B;
	color:white;
	font-weight:bold; 
}

